{
   "containerDefinitions": [ 
      { 
         "essential": true,
          "image": "06640050dc3f556bb.dkr.ecr.us-east-1.amazonaws.com/jenkins",
          "logConfiguration": {
            "logDriver": "awslogs",
            "options": { 
               "awslogs-group" : "/ecs/jenkins",
               "awslogs-region": "us-east-1",
               "awslogs-stream-prefix": "ecs"
            }
         },
         "name": "jenkins",
         "entryPoint": [],
         "command": [],
         "portMappings": [ 
            { 
               "containerPort": 80,
               "hostPort": 80,
               "protocol": "tcp"
            }
         ]
      }
   ],
   "cpu": "256",
   "executionRoleArn": "arn:aws:iam::792808453918:role/ecsExecutionRole",
   "family": "jenkins",
   "memory": "512",
   "networkMode": "awsvpc",
   "requiresCompatibilities": [ 
       "FARGATE" 
    ]
}
