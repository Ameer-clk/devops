{
   "containerDefinitions": [ 
      { 
         "essential": true,
          "image": "ami-007855ac798b75e.dkr.ecr.us-east-1.amazonaws.com/:cloudstudylatest",
          "logConfiguration": {
            "logDriver": "awslogs",
            "options": { 
               "awslogs-group" : "/ecs/cloudstudy",
               "awslogs-region": "us-east-1",
               "awslogs-stream-prefix": "ecs"
            }
         },
         "name": "cloudstudy",
         "entryPoint": [],
         "command": [],
         "portMappings": [ 
            { 
               "containerPort": 80,
               "hostPort": 80,
               "protocol": "tcp"
            }
         ]
      }
   ],
   "cpu": "256",
   "executionRoleArn": "arn:aws:iam::098386855668:role/ecsTaskExecutionRole",
   "family": "cloudstudy",
   "memory": "512",
   "networkMode": "awsvpc",
   "requiresCompatibilities": [ 
       "FARGATE" 
    ]
}
